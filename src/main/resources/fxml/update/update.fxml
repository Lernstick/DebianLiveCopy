                                       </children>
                                       <VBox.margin>
                                          <Insets left="20.0" />
                                       </VBox.margin>
                                    </HBox>
                                    <CheckBox fx:id="chbAutomaticBackupRemove" disable="true" minHeight="32.0" mnemonicParsing="false" text="%update.chbAutomaticBackupRemove">
